#include <iostream>
#include<climits>
#include<vector>
using namespace std;
bool isPalPer(string str){
vector<int> freq(26,0);
for(int i =0; i<str.size(); i++){
	freq[str[i]-'a']++;
}
int oddCount = 0;
for(int i = 0; i<26;i++){
 if(freq[i]%2 == 1){
   oddCount++;
	}
}
return (oddCount <=1);
}


int main(){
	string str;
	cin >> str;
    if(isPalPer(str)){
		cout << "YES"<<endl;
	}else{
		cout << "NO"<<endl;
	}
}
