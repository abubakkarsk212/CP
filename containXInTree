bool isPresent(TreeNode<int>* root, int x) {
    // Write your code here
    if(root->data == x){
        return true;
    }
    for(int i =0; i<root->children.size();i++){
        bool isP = isPresent(root->children[i],x);
        if(isP == true){
            return true;
        }
    }
    return false;
}
