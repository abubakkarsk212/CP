Node* insertNode(Node *head, int i, int data) {
	// Write your code here
    if(head == NULL){
        return head;
    }
    
    if(i == 0){
        Node *newNode = new Node(data);
        newNode->next = head;
        head = newNode;
        return head;
    }
    Node *x = insertNode(head->next,i-1,data);
    head->next = x;
    return head;
}
