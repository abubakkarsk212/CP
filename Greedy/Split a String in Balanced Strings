 int balancedStringSplit(string s) {
        int balancedC = 0;
        int count = 0;
        for(int i =0; i<s.size();i++){
            char ch = s.at(i);
            if(ch == 'L'){
                count++;
            }else if(ch == 'R'){
                count--;
            }
            if(count == 0){
                balancedC++;
            }
        }
        return balancedC;
    }
