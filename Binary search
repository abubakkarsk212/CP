int binarySearch(int *input, int n, int val)
{
    //Write your code here
    int left = 0;
    int end = n-1;
    while(left <= end){
        int mid = (left + end)/2;
        if(input[mid] == val){
            return mid;
        }
        else if(input[mid]>val){
            end = mid - 1;
        }
        else{
            left = mid +1;
        }
    }
    return -1;
}
